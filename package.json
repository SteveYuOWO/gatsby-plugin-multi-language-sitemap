{
  "name": "gatsby-plugin-sitemap",
  "description": "Gatsby plugin that automatically creates a sitemap for your site",
  "version": "1.0.0",
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@types/jest": "^26.0.24",
    "common-tags": "^1.8.0",
    "jest": "^27.0.6",
    "minimatch": "^3.0.4",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "babel-preset-gatsby-package": "^1.10.0-next.0",
    "cross-env": "^7.0.3",
    "gatsby": "^3.0.0-next.0",
    "gatsby-plugin-utils": "^1.10.0-next.0",
    "git-cz": "^4.7.6",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  },
  "scripts": {
    "start": "node index",
    "clean": "rm -rf public build",
    "build": "babel src --out-dir ./ --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "cz": "git-cz"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}